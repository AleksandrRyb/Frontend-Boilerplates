{
  "name": "app-typescript",
  "description": "Stable version of a basic TypeScript example with SCSS.",
  "license": "MIT",
  "version": "2.4.1",
  "scripts": {
    "prod": "webpack --env.NODE_ENV=production",
    "dev": "webpack --env.NODE_ENV=development",
    "start": "webpack-dev-server --env.NODE_ENV=development",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest",
    "test:e2e:cypress": "cypress open",
    "test:e2e": "start-server-and-test start http-get://localhost:4000 test:e2e:cypress",
    "lint": "npm run lint:scripts && npm run lint:styles",
    "lint:scripts": "eslint --fix . --ext .ts src",
    "lint:styles": "stylelint \"./src/**/*.scss\" --fix"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "bootstrap": "^4.3.1",
    "jquery": "3.4.1",
    "normalize-scss": "7.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/register": "^7.4.4",
    "@cypress/webpack-preprocessor": "^4.1.0",
    "@types/jest": "^24.0.15",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "autoprefixer": "^9.6.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "cypress": "^3.4.0",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "jest-transform-stub": "^2.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "prettier-stylelint": "^0.4.2",
    "require.all": "^2.0.4",
    "sass-loader": "^7.1.0",
    "start-server-and-test": "^1.9.1",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-scss": "^3.9.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.5.3",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "repository": "https://github.com/CKGrafico/Frontend-Boilerplates#frontend-boilerplates",
  "author": "https://github.com/CKGrafico/Frontend-Boilerplates#-contributors"
}
