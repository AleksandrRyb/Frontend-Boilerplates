{
  "name": "app-nuxt",
  "description": "Stable version of a basic Nuxt with TypeScript and Express",
  "license": "MIT",
  "version": "2.6.0",
  "scripts": {
    "predev": "npm run fonts",
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "debug": "cross-env NODE_ENV=development nodemon --inspect server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "fonts": "fontmin client/assets/fonts/**/* client/assets/.fonts/",
    "test:unit": "node test/scripts/unit-nuxt-configs.js",
    "test:e2e": "node test/scripts/e2e-nuxt-configs.js",
    "test": "npm run test:unit && npm run test:e2e"
  },
