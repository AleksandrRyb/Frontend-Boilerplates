{
  "name": "myapp",
  "version": "1.1.0",
  "scripts": {
   "watchify": "mkdirp debug && watchify -vd -p tsify -p browserify-hmr -e app/ts/index.ts -o dist/js/app.js",
    "serve": "http-server -c 1 -a localhost -o",
    "start": "npm-run-all --parallel watchify watch",
    "dev": "gulp --env=development",
    "prod": "gulp --env=production",
    "watch": "gulp --env=development watcher"
  },
  "jest": {
    "transform": {
      ".*\\.(ts|vue)$": "<rootDir>/node_modules/vue-typescript-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "vue"
    ],
    "testRegex": "/test/.*\\.(ts|js)$",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/.*\\.(ts|js)$",
      "/.*\\.vue$"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "del": "^2.2.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-clean-css": "^2.3.2",
    "gulp-connect": "^5.0.0",
    "gulp-environment": "^1.3.0",
    "gulp-fontmin": "^0.7.4",
    "gulp-imagemin": "^3.1.1",
    "gulp-load-plugins": "^1.4.0",
    "gulp-newer": "^1.3.0",
    "gulp-noop": "^1.0.0",
    "gulp-sass": "^2.3.2",
    "gulp-sass-lint": "^1.3.2",
    "gulp-sourcemaps": "^1.9.1",
    "gulp-tslint": "^7.1.0",
    "gulp-typescript": "^3.1.5",
    "gulp-uglify": "^2.0.0",
    "opn": "^4.0.2",
    "require.all": "^2.0.3",
    "rollup": "^0.41.5",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-inject": "^2.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-string": "^2.0.2",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-vue": "^2.2.21",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1",
  "@types/jest": "^16.0.0",
    "babel-core": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-runtime": "^6.0.0",
    "browserify": "^13.1.0",
    "browserify-hmr": "^0.3.1",
    "cross-env": "^3.1.2",
    "envify": "^4.0.0",
    "html2jade": "^0.8.5",
    "http-server": "^0.9.0",
    "jest-cli": "^18.0.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.0.0",
    "pug": "^2.0.0-beta5",
    "rplcr": "^0.2.2",
    "tsify": "^3.0.0",
    "tslint": "^4.2.0",
    "tslint-config-locoslab": "^1.0.0",
    "typescript": "^2.1.4",
    "uglify-js": "^2.5.0",
    "vue": "^2.1.8",
    "vue-jest-utils": "^0.2.0",
    "vue-typescript-component": "^0.6.1",
    "vue-typescript-import-dts": "^2.0.0",
    "vue-typescript-jest": "^0.3.0",
    "vueify": "^9.4.1",
    "watchify": "^3.4.0"
  },
  "browserify": {
    "transform": [
      "vueify"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "normalize-scss": "^6.0.0",
    "vue": "^2.2.3",
    "vue-class-component": "^5.0.0",
    "vue-router": "^2.3.0"
  },
  "description": "Boilerplate with Gulp + SASS and TypeScript with Vue",
