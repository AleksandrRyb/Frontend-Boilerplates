{
  "name": "app-cypress",
  "description": "Stable version of a basic Cypress example with TypeScript.",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "start": "npm start --prefix ./_example-project/ClientApp",
    "test": "start-server-and-test start http-get://localhost:3000 \"cypress run\"",
    "open": "start-server-and-test start http-get://localhost:3000 \"cypress open\"",
    "lint": "npm run lint:scripts && npm run lint:editor",
    "lint:scripts": "eslint --fix ./specs --ext .ts",
    "lint:editor": "eclint fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "cypress": "^4.12.1",
    "eclint": "^2.8.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "prettier-stylelint": "^0.4.2",
    "start-server-and-test": "^1.11.0",
    "typescript": "3.8.3"
  },
  "eslintConfig": {
    "extends": [
      "./.eslintrc.js"
    ]
  },
  "repository": "https://github.com/CKGrafico/Frontend-Boilerplates#frontend-boilerplates",
  "author": "https://github.com/CKGrafico/Frontend-Boilerplates#-contributors"
}
